	<queryString>
		<![CDATA[SELECT a.company_name,a.address,a.phone,a.mobile,a.email,a.fax,a.website,a.wh_req_id, b.ud_sample_no ,a.test_report_no,a.mat_unit_name,
b.material_name,b.sample_details, b.from_dept_nm,b.receive_at,b.sample_id, b.id sample_rcv_id,
(CAST(b.st_qty as float) + CAST((SELECT ret_qty from lims_ret_sample_rcv_infos where sample_id ::text =$P{sampleId} ::text ) as float)) rcv_qty,b.st_unit_name,b.manufacture_name,
(Select remarks from lims_tm_verify_infos where sample_rcv_id::text=b.id::text) remarks,
(Select opinion from lims_lm_verify_infos where sample_rcv_id::text=b.id::text) opinion,
string_agg(c.batch_no, ', ') AS batch,a.verify_by,
string_agg(TO_CHAR( c.mfg_date, 'dd-Mon-yyyy' ), ',')  AS mfg_date,
string_agg(TO_CHAR( c.exp_date, 'dd-Mon-yyyy' ), ',') AS exp_date,
(CASE 
      WHEN b.IS_DECISION='Y'  THEN (SELECT full_name ||E'\n'|| designation  FROM lims_users where id=(SELECT CREATED_BY FROM lims_tm_verify_infos where sample_rcv_id=b.id) )
      ELSE (SELECT full_name ||E'\n'|| designation  FROM lims_users where id=a.verify_by )
END) verify_nm,
(CASE 
      WHEN b.IS_DECISION='Y'  THEN (SELECT full_name ||E'\n'|| designation  FROM lims_users where id=(SELECT CREATED_BY FROM lims_lm_verify_infos where sample_rcv_id=b.id) )
      ELSE (SELECT full_name ||E'\n'|| designation  FROM lims_users where id=(SELECT CREATED_BY FROM lims_tm_verify_infos where sample_rcv_id=b.id) )
END) check_by 
from view_sample_infos a
LEFT JOIN view_sample_rcv_infos b 
ON a.sample_id::text=b.sample_id::text
FULL OUTER JOIN lims_wh_request_details_infos c
on c.wh_request_id::text=b.wh_request_id::text
WHERE    a.sample_id ::text= $P{sampleId} ::text
group by a.verify_by,a.company_name,a.address,a.phone,a.mobile,a.email,a.fax,a.website,a.wh_req_id, b.ud_sample_no ,
b.material_name,b.sample_details, b.from_dept_nm,b.receive_at,b.sample_id,a.test_report_no,a.mat_unit_name,
b.id,b.st_qty,b.ret_qty,b.st_unit_name,b.manufacture_name,b.IS_DECISION;]]>
	</queryString>
