	<parameter name="reportDate" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT a.company_name,a.address,a.phone,a.mobile,a.email,a.fax,a.website,a.wh_req_id,
b.ud_sample_no ,a.test_report_no,a.mat_unit_name,
CONCAT(LEFT(a.test_report_no, 6),TO_CHAR( $P{reportDate}::DATE , 'YYMM'),'-',RIGHT(b.ud_sample_no, 4))  report_no_new,
b.material_name,b.sample_details, b.from_dept_nm,b.receive_at,b.sample_id, b.id sample_rcv_id,
CAST(b.st_qty as float) as rcv_qty,b.st_unit_name,b.manufacture_name,
(Select remarks from lims_tm_verify_infos where sample_rcv_id::text=b.id::text) remarks,
(select note from lims_tm_verify_infos where  sample_rcv_id::text=b.id::text) note,
(Select opinion from lims_lm_verify_infos where sample_rcv_id::text=b.id::text) opinion,
(select client_name from lims_client_infos where ud_client_id in (select client_id from view_sample_infos where sample_id ::text =a.sample_id::text )) client_name,
(select address from lims_client_infos where ud_client_id in (select client_id from view_sample_infos where sample_id ::text =a.sample_id ::text )) client_address,
(select email from lims_client_infos where ud_client_id in (select client_id from view_sample_infos where sample_id ::text =a.sample_id ::text )) client_email,
(select mobile_no from lims_client_infos where ud_client_id in (select client_id from view_sample_infos where sample_id ::text =a.sample_id::text )) client_tel,
string_agg(c.batch_no, ', ') AS batch,a.verify_by,
string_agg(TO_CHAR( c.mfg_date, 'dd-Mon-yyyy' ), ',')  AS mfg_date,
string_agg(TO_CHAR( c.exp_date, 'dd-Mon-yyyy' ), ',') AS exp_date,
(Select count(*) from view_result_status_info where  iso_scope='Y' and sample_id ::text =a.sample_id ::text  ) is_iso,
(CASE 
      WHEN b.IS_DECISION='Y'  THEN (SELECT full_name ||E'\n'|| designation  FROM lims_users where id=(SELECT CREATED_BY FROM lims_tm_verify_infos where sample_rcv_id=b.id) )
      ELSE (SELECT full_name ||E'\n'|| designation  FROM lims_users where id=a.verify_by )
END) verify_nm_pre,
(CASE 
 when (b.is_chemical='Y' and b.is_microbiological='N') THEN (SELECT full_name ||E'\n'|| designation  FROM lims_users where id=b.chemi_dist_by)
 when (b.is_chemical='N' and b.is_microbiological='Y') THEN (SELECT full_name ||E'\n'|| designation  FROM lims_users where id=b.micro_dist_by)
 else  (SELECT full_name ||E'\n'|| designation  FROM lims_users where id=b.chemi_dist_by) 
 END)verify_nm,
 (CASE 
 when (b.is_chemical='Y' and b.is_microbiological='N') THEN fnc_employee_sign(b.chemi_dist_by)
 when (b.is_chemical='N' and b.is_microbiological='Y') THEN fnc_employee_sign(b.micro_dist_by)
 else  fnc_employee_sign(b.chemi_dist_by)
 END)verify_sign,
 (CASE 
 when (b.is_chemical='Y' and b.is_microbiological='N') THEN fnc_verify_date(a.sample_id,'C')
 when (b.is_chemical='N' and b.is_microbiological='Y') THEN fnc_verify_date(a.sample_id,'M')
 else  fnc_verify_date(a.sample_id,'C')
 END) verify_date,
 (CASE 
      WHEN b.IS_DECISION='Y'  THEN fnc_employee_sign((SELECT CREATED_BY FROM lims_lm_verify_infos where sample_rcv_id=b.id))
      ELSE fnc_employee_sign((SELECT CREATED_BY FROM lims_tm_verify_infos where sample_rcv_id=b.id))
  
END) check_by_sign,
(CASE 
      WHEN b.IS_DECISION='Y'  THEN (SELECT date(CREATED_AT) FROM lims_lm_verify_infos where sample_rcv_id=b.id) 
      ELSE (SELECT date(CREATED_AT) FROM lims_tm_verify_infos where sample_rcv_id=b.id) 
END) check_by_date,
(CASE 
      WHEN b.IS_DECISION='Y'  THEN (SELECT full_name ||E'\n'|| designation  FROM lims_users where id=(SELECT CREATED_BY FROM lims_lm_verify_infos where sample_rcv_id=b.id) )
      ELSE (SELECT full_name ||E'\n'|| designation  FROM lims_users where id=(SELECT CREATED_BY FROM lims_tm_verify_infos where sample_rcv_id=b.id) )
END) check_by ,b.condition_type,    b.abnormal_type,    b.abnormal_desc,    b.customer_desc,fnc_test_date(a.sample_id,'S')  test_start_date,fnc_test_date(a.sample_id,'E')  test_end_date
from view_sample_infos a
LEFT JOIN view_sample_rcv_infos b 
ON a.sample_id::text=b.sample_id::text
FULL OUTER JOIN lims_wh_request_details_infos c
on c.wh_request_id::text=b.wh_request_id::text
WHERE    a.sample_id ::text= $P{sampleId} ::text
group by a.verify_by,a.company_name,a.address,a.phone,a.mobile,a.email,a.fax,a.website,a.wh_req_id, b.ud_sample_no ,
b.material_name,b.sample_details, b.from_dept_nm,b.receive_at,b.sample_id,a.test_report_no,a.mat_unit_name,b.is_chemical,b.is_microbiological,b.chemi_dist_by,b.micro_dist_by,
b.id,b.st_qty,b.ret_qty,b.st_unit_name,b.manufacture_name,b.IS_DECISION,a.sample_id,b.condition_type,    b.abnormal_type,    b.abnormal_desc,    b.customer_desc;]]>
	</queryString>
